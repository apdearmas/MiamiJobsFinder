  @model IPagedList<JobOfferListViewModel>
@{
    ViewBag.Title = "Search";
}


<div class="row col-md-10">
    <form method="get" action="@Url.Action("Index")"
          data-mjf-ajax="true" data-mjf-target="#jobList" class="navbar-form navbar-left" role="search">

        <div class="form-group ">
            <input id="enterjobtitle" type="text" name="searchTerm" class="form-control " placeholder="Enter job title..."
                   data-mjf-autocomplete="@Url.Action("Autocomplete")" autofocus="autofocus">
        </div>
        <div class="form-group ">
            <button type="submit" class="btn btn-default" contenteditable="false"><i class="glyphicon glyphicon-search"></i></button>
        </div>

    </form>

</div>

@Html.Partial("_Jobs", Model)








