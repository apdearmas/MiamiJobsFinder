@model IPagedList<JobOfferListViewModel>


    @{
        ViewBag.Title = "Job Search";
    }

    <hgroup class="title">
        <h1>@ViewBag.Title</h1>
    </hgroup>



    <form method="get" action="@Url.Action("Index")"
          data-mjf-ajax="true" data-mjf-target="#jobList">

        <input type="search" name="searchTerm" data-mjf-autocomplete="@Url.Action("Autocomplete")" />
        <input type="submit" value="Find Jobs" />
    </form>

    @Html.Partial("_Jobs", Model)


